<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Thortful Test Regression Suite" verbose="4">


    <listeners>
		<listener
			class-name="com.qa.thortful.listeners.ExtentReportListener" />
	</listeners>
     <test name="Thortful Home Page Test_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.thortful.tests.HomePageTest" />
		</classes>
	 </test>
	 <test name="Thortful Home Page Test_chromium">
		<parameter name="browser" value="chromium" />
		<classes>
			<class name="com.qa.thortful.tests.HomePageTest" />
		</classes>
	</test>
	

</suite>